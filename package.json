{
  "name": "@kurone-kito/jsonresume-theme-japanese-cv-style-docx",
  "version": "2.0.0",
  "description": "Generate a Word docx file of Japanese-style resume (職務経歴書) from JSON Resume",
  "keywords": [
    "Curriculum Vitae",
    "CV",
    "documents",
    "docx",
    "employment",
    "Japanese",
    "JSONResume",
    "JSON Resume",
    "library",
    "Microsoft Word",
    "msword",
    "portfolio",
    "resume",
    "Themes",
    "職務経歴書",
    "日本語",
    "履歴書"
  ],
  "homepage": "https://github.com/kurone-kito/jsonresume-theme-japanese-cv-style-docx#readme",
  "bugs": {
    "url": "https://github.com/kurone-kito/jsonresume-theme-japanese-cv-style-docx/issues"
  },
  "license": "MIT",
  "author": "kurone-kito <krone@kit.black> (https://kit.black/)",
  "main": "dist/index.js",
  "repository": "github:kurone-kito/jsonresume-theme-japanese-cv-style-docx",
  "scripts": {
    "build:doc": "typedoc --exclude '**/*.spec.ts' --out ./docs/ ./src/",
    "build:resume": "dotenv -- node sample/build.js",
    "prepack": "webpack",
    "prepare": "run-s prepare:*",
    "prepare:dotenv": "node -e \"require('semver').satisfies(process.version, '<13') && console.log('NODE_ICU_DATA=' + require('full-icu').datPath())\" > .env",
    "prepare:husky": "husky install",
    "semantic-release": "semantic-release",
    "start": "webpack --watch",
    "test": "run-p test:lint test:jest",
    "test:jest": "dotenv -- jest --coverage=true",
    "test:lint": "eslint --cache --fix .",
    "test:coveralls": "dotenv -- jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:watch": "dotenv -- jest --coverage --watch"
  },
  "dependencies": {
    "@kurone-kito/jsonresume-types": "^0.2.3",
    "docx": "^6.0.3",
    "inversify": "^5.1.1",
    "lodash.groupby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "resume-schema": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.0.0",
    "@commitlint/config-conventional": "^13.0.0",
    "@octokit/core": "^3.5.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@types/jest": "^26.0.23",
    "@types/json5": "^2.2.0",
    "@types/lodash.chunk": "^4.2.6",
    "@types/lodash.groupby": "^4.6.6",
    "@types/lodash.sortby": "^4.7.6",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "coveralls": "^3.1.1",
    "dotenv-cli": "^4.0.0",
    "dts-bundle-webpack": "^1.0.2",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-webpack-plugin": "^2.5.4",
    "full-icu": "^1.3.4",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "json5": "^2.2.0",
    "lodash.chunk": "^4.2.0",
    "marked": "^2.1.3",
    "npm-run-all2": "^5.0.2",
    "prettier": "^2.3.2",
    "semantic-release": "^19.0.3",
    "semantic-release-commitlint": "^1.2.2",
    "semver": "^7.3.5",
    "showdown": "^1.9.1",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths-jest": "0.0.1",
    "typedoc": "^0.21.2",
    "typescript": "^4.3.5",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2",
    "xml": "^1.0.1"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  }
}
