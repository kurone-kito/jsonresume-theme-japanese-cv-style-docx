{
  "name": "@kurone-kito/jsonresume-theme-japanese-cv-style-docx",
  "version": "1.0.1",
  "description": "Generate a Word docx file of Japanese-style resume (職務経歴書) from JSON Resume",
  "author": "kurone-kito",
  "license": "MIT",
  "keywords": [
    "Curriculum Vitae",
    "CV",
    "documents",
    "docx",
    "employment",
    "Japanese",
    "JSONResume",
    "JSON Resume",
    "library",
    "Microsoft Word",
    "msword",
    "portfolio",
    "resume",
    "Themes",
    "職務経歴書",
    "日本語",
    "履歴書"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kurone-kito/jsonresume-theme-japanese-cv-style-docx.git"
  },
  "bugs": {
    "url": "https://github.com/kurone-kito/jsonresume-theme-japanese-cv-style-docx/issues"
  },
  "homepage": "https://github.com/kurone-kito/jsonresume-theme-japanese-cv-style-docx#readme",
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "build:doc": "typedoc --exclude '**/*.spec.ts' --out ./docs/ ./src/",
    "build:resume": "NODE_ICU_DATA=$(node-full-icu-path) node sample/build.js",
    "prepack": "webpack",
    "semantic-release": "semantic-release",
    "start": "webpack --watch",
    "test": "run-p test:lint test:jest",
    "test:jest": "NODE_ICU_DATA=$(node-full-icu-path) jest --coverage",
    "test:lint": "eslint --fix './**/*'",
    "test:coveralls": "NODE_ICU_DATA=$(node-full-icu-path) jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:watch": "NODE_ICU_DATA=$(node-full-icu-path) jest --coverage --watch"
  },
  "engines": {
    "node": ">=12.20"
  },
  "dependencies": {
    "@kurone-kito/jsonresume-types": "^0.2.1",
    "docx": "^6.0.3",
    "inversify": "^5.0.5",
    "lodash.groupby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "resume-schema": "^1.0.0",
    "ts-polyfill": "^3.8.2"
  },
  "devDependencies": {
    "@jedmao/semantic-release-npm-github-config": "^1.0.9",
    "@octokit/core": "^3.4.0",
    "@types/jest": "^26.0.22",
    "@types/json5": "^2.2.0",
    "@types/lodash.chunk": "^4.2.6",
    "@types/lodash.groupby": "^4.6.6",
    "@types/lodash.sortby": "^4.7.6",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "coveralls": "^3.1.0",
    "dts-bundle-webpack": "^1.0.2",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-webpack-plugin": "^2.5.3",
    "full-icu": "^1.3.4",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "json5": "^2.2.0",
    "lodash.chunk": "^4.2.0",
    "marked": "^0.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "semantic-release": "^15.14.0",
    "showdown": "^1.9.1",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths-jest": "0.0.1",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.4",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0",
    "xml": "^1.0.1"
  }
}
