{
  "name": "@kurone-kito/jsonresume-theme-japanese-cv-style-docx",
  "version": "1.0.1",
  "description": "Generate a Word docx file of Japanese-style resume (職務経歴書) from JSON Resume",
  "author": "kurone-kito",
  "license": "MIT",
  "keywords": [
    "Curriculum Vitae",
    "CV",
    "documents",
    "docx",
    "employment",
    "Japanese",
    "JSONResume",
    "JSON Resume",
    "library",
    "Microsoft Word",
    "msword",
    "portfolio",
    "resume",
    "Themes",
    "職務経歴書",
    "日本語",
    "履歴書"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kurone-kito/jsonresume-theme-japanese-cv-style-docx.git"
  },
  "bugs": {
    "url": "https://github.com/kurone-kito/jsonresume-theme-japanese-cv-style-docx/issues"
  },
  "homepage": "https://github.com/kurone-kito/jsonresume-theme-japanese-cv-style-docx#readme",
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "build:doc": "typedoc --exclude '**/*.spec.ts' --mode modules --out ./docs/ ./src/",
    "build:resume": "NODE_ICU_DATA=$(node-full-icu-path) node sample/build.js",
    "prepack": "webpack",
    "semantic-release": "semantic-release",
    "start": "webpack --watch",
    "test": "run-p test:lint test:jest",
    "test:jest": "NODE_ICU_DATA=$(node-full-icu-path) jest --coverage",
    "test:lint": "eslint --fix './**/*'",
    "test:coveralls": "NODE_ICU_DATA=$(node-full-icu-path) jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:watch": "NODE_ICU_DATA=$(node-full-icu-path) jest --coverage --watch"
  },
  "engines": {
    "node": ">=12.20"
  },
  "dependencies": {
    "@kurone-kito/jsonresume-types": "^0.1.1",
    "docx": "^5.0.0-rc7",
    "inversify": "^5.0.1",
    "lodash.groupby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "resume-schema": "0.0.18",
    "ts-polyfill": "^3.5.3"
  },
  "devDependencies": {
    "@jedmao/semantic-release-npm-github-config": "^1.0.8",
    "@types/jest": "^24.0.23",
    "@types/json5": "0.0.30",
    "@types/lodash.chunk": "^4.2.6",
    "@types/lodash.groupby": "^4.6.6",
    "@types/lodash.sortby": "^4.7.6",
    "@types/webpack": "^4.41.0",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "coveralls": "^3.0.7",
    "dts-bundle-webpack": "^1.0.2",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-prettier": "^3.1.1",
    "full-icu": "^1.3.0",
    "husky": "^4.0.0-beta.5",
    "jest": "^24.9.0",
    "json5": "^2.1.1",
    "lodash.chunk": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "semantic-release": "^15.13.31",
    "showdown": "^1.9.1",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.2",
    "tsconfig-paths-jest": "0.0.1",
    "typedoc": "^0.15.2",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "xml": "^1.0.1"
  }
}
